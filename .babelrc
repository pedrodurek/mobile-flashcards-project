{
    "presets": ["babel-preset-expo"],
    "env": {
        "development": {
            "plugins": ["transform-react-jsx-source"]
        }
    },
    "plugins": [
        [
            "module-resolver",
            {
                "root": ["./"],
                "alias": {
                    "@actions": "./actions",
                    "@components": "./components",
                    "@reducers": "./reducers",
                    "@utils": "./utils",
                    "@colors": "./utils/colors.js",
                    "@styles": "./utils/styles.js",
                    "@helper": "./utils/helper.js"
                }
            }
        ]
    ]
}
